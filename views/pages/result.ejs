<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="../styles/style.css">
    <title>result</title>
</head>
<body>
    <header>
        <h1>
            search results 
        </h1>
    </header>
    <main>

<div class="allContainer">
<%data.forEach(val=>{%>

<div class="container">
<h3><%=val.name%></h3>
<h4><%=val.country%></h4>
<img src="<%=val.image%>" alt="">
<p><%=val.overview%></p>

<div>
<button class="show">SELECT</button>
<form action="/addToDb" method="POST" class="form">
    <input type="text"  name="name" value="<%=val.name%>" >
    <input type="text"  name="country" value="<%=val.country%>" >
    <input type="text"  name="image" value="<%=val.image%>" >
    <input type="text"  name="overview" value="<%=val.overview%>" >
    <input type="submit"   value="ADD TO LIST" >

</form>

</div>

</div>

<%})%>


</div>

 </main>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="../js/app.js"></script>
</body>
</html>